function calculatetime()
local part = createTab("time")
coroutine.wrap(function()
repeat wait()

local t = os.time()	
part:FindFirstChild("BillboardGui").TextLabel.Text = ("%.2d:%.2d:%.2d"):format(t/3600%24,t/60%60,t%60)    	
until part == nil or part.Parent ==nil
end)()	
end

